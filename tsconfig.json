{
	"compilerOptions": {
		"target": "es2022",
		"module": "Node16",
		"moduleResolution": "node16",
		"lib": ["es2022", "dom"],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "@isopodlabs/vscode_utils",
		"forceConsistentCasingInFileNames": true,
		"rewriteImports": true,
		"baseUrl": "src",
		"paths": {
			"shared/*": [
				"../shared/src/*"
			]
		}
	},
	"include": ["src/extension.ts"],
  }
  