{
	"name": "deepseek-chat",
	"displayName": "Deepseek Chat",
	"description": "Chat with deepseek",
	"version": "0.0.0",
	"publisher": "isopodlabs",
	"license": "MIT",
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [],
	"activationEvents": [],
	"main": "./out/src/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/adrianstephens/vscode-deepseek2.git"
	},
	"files": [
		"out/**/*.js",
		"assets/icon.png",
		"LICENSE.txt"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "deepseek2-activitybar",
					"title": "DeepSeek",
					"icon": "assets/deepseek.svg"
				}
			]
		},
		"views": {
			"deepseek2-activitybar": [
				{
					"id": "deepseek2-view",
					"name": "DeepSeek",
					"type": "webview"
				}
			]
		},
		"commands": [
			{
				"command": "deepseek2.chat",
				"title": "Deepseek Chat"
			},
			{
				"command": "deepseek2.clear",
				"title": "Clear",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "deepseek2.clear",
					"when": "view == deepseek2-view",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "DeepSeek",
			"properties": {
				"deepseek2.server": {
					"type": "string",
					"default": "http://threadripper.local:11434",
					"description": "API Server"
				},
				"deepseek2.model": {
					"type": "string",
					"default": "deepseek-r1:32b",
					"description": "Set the model"
				},
				"deepseek2.apikey": {
					"type": "string",
					"default": "sk-kgahvlalrbfjyftxrciniliopeblhxsgrxebrwgiqwwxwxth",
					"description": "Set the API key"
				},
				"deepseek2.temperature": {
					"type": "string",
					"default": "0",
					"description": "Set the API temperature, between 0 and 2"
				},
				"deepseek2.max_tokens": {
					"type": "string",
					"default": "4096",
					"description": "Set the max_tokens of the API"
				},
				"deepseek2.lang": {
					"type": "string",
					"default": "en",
					"description": "Set the language"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"package": "vsce package",
		"publish": "vsce publish patch"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.9",
		"@types/node": "20.x",
		"@types/vscode": "^1.80.0",
		"typescript": "npm:@isopodlabs/typescript@^5.8.0",
		"@typescript-eslint/eslint-plugin": "^8.10.0",
		"@typescript-eslint/parser": "^8.7.0",
		"@vscode/codicons": "^0.0.36"
	},
	"dependencies": {
		"@types/katex": "^0.16.7",
		"katex": "^0.16.21",
		"openai": "^4.83.0"
	},
	"overrides": {
		"typescript": "npm:@isopodlabs/typescript@^5.8.0"
	}
}
